<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spjzweb.dao.ThreadAcceptanceCriteriaDao">

    <insert id="addThreadAcceptanceCriteria" parameterType="com.spjzweb.entity.ThreadAcceptanceCriteria">
    INSERT INTO threading_acceptance_criteria(thread_acceptance_criteria_no,thread_pitch_diameter_max,thread_pitch_diameter_min,thread_sealing_surface_diameter_max,thread_sealing_surface_diameter_min,thread_sealing_surface_ovality_max,thread_sealing_surface_ovality_min,thread_pitch_max,thread_pitch_min,thread_taper_max,thread_taper_min,thread_height_max,thread_height_min,thread_bearing_surface_width_max,thread_bearing_surface_width_min,couping_inner_end_depth_max,couping_inner_end_depth_min,thread_hole_inner_diameter_max,thread_hole_inner_diameter_min,couping_od_max,couping_od_min,couping_length_max,couping_length_min,last_update_time) VALUES (#{thread_acceptance_criteria_no},#{thread_pitch_diameter_max},#{thread_pitch_diameter_min},#{thread_sealing_surface_diameter_max},#{thread_sealing_surface_diameter_min},#{thread_sealing_surface_ovality_max},#{thread_sealing_surface_ovality_min},#{thread_pitch_max},#{thread_pitch_min},#{thread_taper_max},#{thread_taper_min},#{thread_height_max},#{thread_height_min},#{thread_bearing_surface_width_max},#{thread_bearing_surface_width_min},#{couping_inner_end_depth_max},#{couping_inner_end_depth_min},#{thread_hole_inner_diameter_max},#{thread_hole_inner_diameter_min},#{couping_od_max},#{couping_od_min},#{couping_length_max},#{couping_length_min},#{last_update_time})
    </insert>
    <delete id="delThreadAcceptanceCriteria">
        delete from threading_acceptance_criteria WHERE
        <foreach collection="array" item="item" open="id in (" separator="," close=")">
           #{item}
        </foreach>
    </delete>
    <update id="updateThreadAcceptanceCriteria" parameterType="com.spjzweb.entity.ThreadAcceptanceCriteria">
    UPDATE threading_acceptance_criteria set thread_acceptance_criteria_no=#{thread_acceptance_criteria_no},thread_pitch_diameter_max=#{thread_pitch_diameter_max},thread_pitch_diameter_min=#{thread_pitch_diameter_min},thread_sealing_surface_diameter_max=#{thread_sealing_surface_diameter_max},thread_sealing_surface_diameter_min=#{thread_sealing_surface_diameter_min},thread_sealing_surface_ovality_max=#{thread_sealing_surface_ovality_max},thread_sealing_surface_ovality_min=#{thread_sealing_surface_ovality_min},thread_pitch_max=#{thread_pitch_max},thread_pitch_min=#{thread_pitch_min},thread_taper_max=#{thread_taper_max},thread_taper_min=#{thread_taper_min},thread_height_max=#{thread_height_max},thread_height_min=#{thread_height_min},thread_bearing_surface_width_max=#{thread_bearing_surface_width_max},thread_bearing_surface_width_min=#{thread_bearing_surface_width_min},couping_inner_end_depth_max=#{couping_inner_end_depth_max},couping_inner_end_depth_min=#{couping_inner_end_depth_min},thread_hole_inner_diameter_max=#{thread_hole_inner_diameter_max},thread_hole_inner_diameter_min=#{thread_hole_inner_diameter_min},couping_od_max=#{couping_od_max},couping_od_min=#{couping_od_min},couping_length_max=#{couping_length_max},couping_length_min=#{couping_length_min},last_update_time=#{last_update_time} where id=#{id}
    </update>


    <select id="getCountAllByLike" resultType="int">
        select count(*) from threading_acceptance_criteria o
        <where>
            <if test="thread_acceptance_criteria_no!=null and thread_acceptance_criteria_no!=''">
                AND o.thread_acceptance_criteria_no LIKE CONCAT('%',#{thread_acceptance_criteria_no},'%')
            </if>
        </where>
    </select>



    <select id="getAllByLike" resultType="com.spjzweb.entity.ThreadAcceptanceCriteria">
        Select o.* from threading_acceptance_criteria o
        <where>
            <if test="thread_acceptance_criteria_no!=null and thread_acceptance_criteria_no!=''">
                AND o.thread_acceptance_criteria_no LIKE CONCAT('%',#{thread_acceptance_criteria_no},'%')
            </if>
        </where>
        ORDER BY o.id DESC
        limit #{skip},#{take}
    </select>
    <select id="getAllDropDownAcceptanceCriteria" resultType="com.spjzweb.entity.ThreadAcceptanceCriteria">
        select o.* from threading_acceptance_criteria o
        <where>
            <if test="thread_acceptance_criteria_no!=null and thread_acceptance_criteria_no!=''">
                AND o.thread_acceptance_criteria_no=#{thread_acceptance_criteria_no}
            </if>
        </where>
    </select>
</mapper>